{
    "name": "digitalcz/register-adries",
    "type": "library",
    "description": "Provides communication with https://data.gov.sk/dataset/register-adries",
    "keywords": [
        "digitalcz",
        "register-adries"
    ],
    "homepage": "https://github.com/digitalcz/register-adries",
    "license": "MIT",
    "authors": [
        {
            "name": "Digital Solutions s.r.o.",
            "email": "info@digital.cz",
            "homepage": "https://digital.cz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.2",
        "ext-json": "*",
        "psr/http-client": "^1.0.0",
        "psr/http-message": "^1.0.0"
    },
    "require-dev": {
        "nyholm/psr7": "^1.3",
        "php-http/mock-client": "^1.3",
        "phpstan/phpstan": "^0.12.25",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "DigitalCz\\RegisterAdries\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DigitalCz\\RegisterAdries\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse",
        "check-style": "phpcs",
        "fix-style": "phpcbf"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
