{
    "name": "digitalcz/register-adries",
    "type": "library",
    "description": "Provides communication with https://data.gov.sk/dataset/register-adries",
    "keywords": [
        "digitalcz",
        "register-adries"
    ],
    "homepage": "https://github.com/digitalcz/register-adries",
    "license": "MIT",
    "authors": [
        {
            "name": "Digital Solutions s.r.o.",
            "email": "info@digital.cz",
            "homepage": "https://digital.cz",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "DigitalCz\\RegisterAdries\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DigitalCz\\RegisterAdries\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
